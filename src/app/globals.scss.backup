.resume-section {
  padding: 60px 0;
  border-bottom: 1px solid rgba(255,255,255,0.08);
}

.resume-section:last-child {
  border-bottom: none;
}

.experience-item {
  padding: 32px 0;
  border-bottom: 1px solid rgba(255,255,255,0.06);
}

.experience-item:last-child {
  border-bottom: none;
}

.achievement-card {
  background: rgba(255,255,255,0.03);
  padding: 24px;
  height: 100%;
}

.skill-category {
  margin-bottom: 24px;
}

.skill-tag {
  display: inline-block;
  background: rgba(255,255,255,0.08);
  padding: 6px 12px;
  margin: 4px;
  font-size: 13px;
  color: rgba(255,255,255,0.8);
}

/* ==========================================
   OFFCANVAS MENU FIXES
   ========================================== */
.tp-offcanvas-2-area ul,
.tp-offcanvas-2-area ol,
.tp-offcanvas-menu ul,
.tp-offcanvas-menu ol {
  list-style: none !important;
  counter-reset: none !important;
}

.tp-offcanvas-2-area li::before,
.tp-offcanvas-menu li::before {
  display: none !important;
  content: none !important;
  counter-increment: none !important;
}

.tp-offcanvas-menu ul li {
  counter-increment: none !important;
}

.tp-offcanvas-menu ul li::before {
  display: none !important;
}

.offcanvas-2-black-bg .fde-menu-link,
.offcanvas-2-black-bg .fde-menu-button,
.offcanvas-2-black-bg .fde-submenu-link {
  color: #ffffff !important;
}

.offcanvas-2-black-bg .fde-submenu li {
  border-bottom-color: rgba(255,255,255,0.15) !important;
}

.offcanvas-2-black-bg .tp-offcanvas-2-right a {
  color: #ffffff !important;
}

.offcanvas-2-black-bg .tp-offcanvas-2-right svg {
  fill: #ffffff !important;
}

@media (max-width: 768px) {
  .tp-offcanvas-2-left {
    overflow-y: auto !important;
    max-height: 100vh !important;
    padding-bottom: 100px !important;
  }
}


/* ==========================================
   FDE CHART SECTION STYLES
   ========================================== */
.fde-section-content {
  width: 100% !important;
}

.fde-chart-section {
  margin-top: 48px !important;
  padding-top: 32px !important;
  border-top: 1px solid #e5e5e5 !important;
}

.fde-chart-header {
  margin-bottom: 24px !important;
}

.fde-chart-header h4 {
  font-size: 18px !important;
  font-weight: 700 !important;
  color: #1a1a1a !important;
  margin: 0 0 8px 0 !important;
}

.fde-chart-header p {
  font-size: 14px !important;
  color: #666 !important;
  margin: 0 !important;
  line-height: 1.5 !important;
}

.fde-chart-container {
  background-color: #ffffff !important;
  border: 1px solid #e5e5e5 !important;
  padding: 24px !important;
  overflow-x: auto !important;
}

/* Ensure charts scale properly */
.fde-chart-container > div {
  max-width: 100% !important;
}

.fde-chart-container svg {
  max-width: 100% !important;
  height: auto !important;
}

/* Chart responsive */
@media (max-width: 1024px) {
  .fde-chart-section {
    margin-top: 32px !important;
    padding-top: 24px !important;
  }
  
  .fde-chart-container {
    padding: 16px !important;
  }
}

@media (max-width: 768px) {
  .fde-chart-section {
    margin-top: 24px !important;
    padding-top: 20px !important;
  }
  
  .fde-chart-header h4 {
    font-size: 16px !important;
  }
  
  .fde-chart-header p {
    font-size: 13px !important;
  }
  
  .fde-chart-container {
    padding: 12px !important;
    margin-left: -12px !important;
    margin-right: -12px !important;
    border-left: none !important;
    border-right: none !important;
  }
}

/* Accordion content needs more max-height for charts */
.fde-accordion-content.open {
  max-height: 4000px !important;
}
