/* Import theme SCSS */
@import '../../public/assets/scss/main.scss';

/* ==========================================
   GLOBAL RESETS
   ========================================== */
* {
  border-radius: 0 !important;
}

/* ==========================================
   OFFCANVAS / HEADER
   ========================================== */
body.offcanvas-open .portfolio-webgl-header {
  opacity: 0 !important;
  pointer-events: none !important;
}

.portfolio-webgl-header {
  position: fixed !important;
  top: 0;
  left: 0;
  right: 0;
  z-index: 100;
  background: transparent !important;
  padding: 20px 40px !important;
  transition: opacity 0.3s ease;
}

.portfolio-webgl-header .header-inner {
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

/* ==========================================
   WebGL SHOWCASE SLIDER STYLES
   ========================================== */
#showcase-slider-holder {
  z-index: 5 !important;
  position: relative !important;
}

#port-showcase-slider-main {
  position: relative;
  z-index: 2;
}

/* Main content container */
.port-showcase-slider-content {
  position: absolute !important;
  top: 300px !important;
  left: 60px !important;
  bottom: auto !important;
  transform: none !important;
  text-align: left !important;
  background-color: rgba(0, 0, 0, 0.65) !important;
  padding: 48px 56px !important;
  max-width: 520px !important;
  z-index: 10 !important;
  color: #ffffff !important;
  backdrop-filter: blur(8px);
  -webkit-backdrop-filter: blur(8px);
}

/* Override the flex container inside */
.port-showcase-slider-content > div {
  display: block !important;
  height: auto !important;
}

/* Remove margin-top: auto behavior */
.port-showcase-slider-content > div > div:last-child {
  margin-top: 32px !important;
}

/* Slider item */
.port-showcase-slider-item {
  height: 100vh;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  padding-top: 0;
}

/* Title styling */
.port-showcase-slider-title-sm span {
  font-family: var(--font-inter), -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif !important;
  font-size: 72px !important;
  font-weight: 700 !important;
  letter-spacing: -3px !important;
  line-height: 0.95 !important;
  color: #ffffff !important;
  display: block !important;
  margin-bottom: 8px !important;
}

.port-showcase-slider-title,
.port-showcase-slider-title span {
  font-family: var(--font-inter), -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif !important;
  color: #ffffff !important;
  opacity: 1 !important;
  visibility: visible !important;
}

/* Subtitle / eyebrow text */
.port-showcase-slider-subtitle {
  font-family: var(--font-inter), -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif !important;
  font-size: 12px !important;
  font-weight: 600 !important;
  text-transform: uppercase !important;
  letter-spacing: 4px !important;
  color: rgba(255, 255, 255, 0.7) !important;
  display: block !important;
  margin-bottom: 20px !important;
}

/* Description text */
.port-showcase-slider-desc {
  font-family: var(--font-inter), -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif !important;
  font-size: 13px !important;
  font-weight: 400 !important;
  color: rgba(255, 255, 255, 0.85) !important;
  line-height: 1.6 !important;
  max-width: 420px !important;
  margin-top: 0 !important;
  margin-bottom: 20px !important;
}

/* Logo in slides */
.port-showcase-slider-logo {
  max-height: 28px !important;
  width: auto !important;
  margin-bottom: 16px !important;
  filter: brightness(0) invert(1);
  opacity: 0.9;
}

/* Company logos grid */
.company-logos-grid {
  display: grid;
  grid-template-columns: repeat(4, 1fr);
  gap: 16px 24px;
  margin-top: 8px;
}

.company-logo-item {
  display: flex;
  align-items: center;
  justify-content: flex-start;
  height: 24px;
}

.company-logo-item img {
  max-width: 100%;
  max-height: 100%;
  width: auto;
  height: auto;
  object-fit: contain;
  filter: brightness(0) invert(1);
  opacity: 0.8;
}

/* Hide default pagination */
.port-showcase-slider .swiper-pagination,
.port-showcase-slider-fraction,
.tp-slider-dot {
  display: none !important;
}

/* Arrow buttons */
.tp-showcase-arrow-box {
  z-index: 20 !important;
  position: absolute !important;
  bottom: 40px !important;
  left: 0 !important;
  right: 0 !important;
  padding: 0 60px !important;
}

.tp-showcase-arrow-box button {
  font-family: var(--font-inter), -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif !important;
  font-size: 13px !important;
  font-weight: 600 !important;
  text-transform: uppercase !important;
  letter-spacing: 2px !important;
  color: #ffffff !important;
  background: transparent !important;
  border: none !important;
  cursor: pointer !important;
  transition: opacity 0.2s ease !important;
}

.tp-showcase-arrow-box button:hover {
  opacity: 0.7 !important;
}

.tp-showcase-arrow-box button i {
  font-size: 12px !important;
  margin: 0 8px !important;
}

/* Tablet responsive */
@media (max-width: 991px) {
  .port-showcase-slider-content {
    left: 32px !important;
    top: 80px !important;
    padding: 32px !important;
    max-width: 400px !important;
  }
  
  .port-showcase-slider-title-sm span {
    font-size: 52px !important;
    letter-spacing: -2px !important;
  }
  
  .tp-showcase-arrow-box {
    padding: 0 32px !important;
    bottom: 32px !important;
  }
}

/* Mobile responsive */
@media (max-width: 768px) {
  .port-showcase-slider-content {
    top: auto !important;
    bottom: 80px !important;
    left: 16px !important;
    right: 16px !important;
    max-width: none !important;
    padding: 24px !important;
  }
  
  .port-showcase-slider-content > div > div:last-child {
    margin-top: 20px !important;
  }
  
  .port-showcase-slider-title-sm span {
    font-size: 40px !important;
    letter-spacing: -1px !important;
    line-height: 1 !important;
  }
  
  .port-showcase-slider-subtitle {
    font-size: 10px !important;
    letter-spacing: 2px !important;
    margin-bottom: 12px !important;
  }
  
  .port-showcase-slider-desc {
    font-size: 13px !important;
    line-height: 1.5 !important;
    margin-bottom: 12px !important;
  }
  
  .port-showcase-slider-logo {
    max-height: 24px !important;
    margin-bottom: 12px !important;
  }
  
  .company-logos-grid {
    grid-template-columns: repeat(4, 1fr);
    gap: 12px 16px;
  }
  
  .company-logo-item {
    height: 20px;
  }
  
  .tp-showcase-arrow-box {
    padding: 0 16px !important;
    bottom: 20px !important;
  }
  
  .tp-showcase-arrow-box button {
    font-size: 12px !important;
    letter-spacing: 1px !important;
  }
}

/* Small mobile */
@media (max-width: 480px) {
  .port-showcase-slider-content {
    bottom: 70px !important;
    padding: 20px !important;
  }
  
  .port-showcase-slider-content > div > div:last-child {
    margin-top: 16px !important;
  }
  
  .port-showcase-slider-title-sm span {
    font-size: 32px !important;
  }
  
  .port-showcase-slider-subtitle {
    font-size: 9px !important;
    letter-spacing: 1.5px !important;
    margin-bottom: 10px !important;
  }
  
  .port-showcase-slider-desc {
    font-size: 12px !important;
  }
  
  .tp-showcase-arrow-box {
    bottom: 16px !important;
  }
}

/* ==========================================
   FDE CASE STUDY PAGE STYLES
   ========================================== */
.fde-page {
  background-color: #ffffff;
  color: #1a1a1a;
  font-family: var(--font-inter), -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
}

.fde-page p {
  color: #4a4a4a;
}

.fde-page h1,
.fde-page h2,
.fde-page h3,
.fde-page h4,
.fde-page h5,
.fde-page h6 {
  color: #1a1a1a;
  font-family: var(--font-inter), -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif;
}

/* Section backgrounds */
.fde-section-white {
  background-color: #ffffff;
}

.fde-section-gray {
  background-color: #f8f9fa;
}

.fde-border-top {
  border-top: 1px solid #e5e5e5;
}

.fde-border-top-thick {
  border-top: 3px solid #1a1a1a;
}

/* Section label */
.fde-section-label {
  font-size: 11px;
  font-weight: 600;
  text-transform: uppercase;
  letter-spacing: 2px;
  color: #999999;
  margin-bottom: 16px;
}

/* Narrative text */
.fde-narrative-text {
  font-size: 16px;
  line-height: 1.7;
  color: #4a4a4a;
  margin-bottom: 16px;
}

/* Text colors */
.fde-text-muted {
  color: #999999 !important;
}

.fde-text-secondary {
  color: #666666 !important;
}

/* Cards */
.fde-card {
  background-color: #ffffff;
  border: 1px solid #e5e5e5;
  padding: 24px;
}

/* Metrics */
.fde-metric-value {
  font-size: 48px;
  font-weight: 800;
  color: #1a1a1a;
  line-height: 1;
  margin-bottom: 8px;
}

.fde-metric-label {
  font-size: 13px;
  color: #999999;
  text-transform: none;
  letter-spacing: 0;
}

/* Pipeline steps */
.fde-pipeline-step {
  display: inline-block;
  background-color: #f0f0f0;
  padding: 6px 12px;
  font-size: 12px;
  font-weight: 500;
  color: #1a1a1a;
}

.fde-pipeline-arrow {
  color: #cccccc;
  margin: 0 4px;
}

/* FDE Page Images */
.fde-page img {
  max-width: 100%;
  height: auto;
}

.fde-page .row {
  display: flex;
  flex-wrap: wrap;
  margin-left: -15px;
  margin-right: -15px;
}

.fde-page .row > [class*="col-"] {
  padding-left: 15px;
  padding-right: 15px;
}

/* FDE Case Study Image Styling */
.fde-case-image {
  width: 100%;
  height: auto;
  object-fit: cover;
}

/* Override Next.js Image component styles in FDE pages */
.fde-page section img {
  display: block;
}

/* FDE Page Mobile Responsive */
@media (max-width: 991px) {
  .fde-page .row.align-items-center {
    flex-direction: column;
  }
  
  .fde-page .col-lg-6,
  .fde-page .col-lg-7,
  .fde-page .col-lg-5 {
    width: 100%;
    max-width: 100%;
  }
  
  .fde-page section img {
    width: 100% !important;
    height: auto !important;
    max-height: 400px !important;
    object-fit: cover !important;
    margin-top: 32px;
  }
  
  .fde-metric-value {
    font-size: 36px;
  }
}

@media (max-width: 768px) {
  .fde-page section {
    padding: 60px 0 !important;
  }
  
  .fde-page section:first-of-type {
    padding-top: 140px !important;
  }
  
  .fde-page h1 {
    font-size: 56px !important;
  }
  
  .fde-page h3 {
    font-size: 22px !important;
  }
  
  .fde-card .row > [class*="col-"] {
    border-right: none !important;
    border-bottom: 1px solid #e5e5e5;
    padding: 16px 0 !important;
  }
  
  .fde-card .row > [class*="col-"]:last-child {
    border-bottom: none;
  }
}

/* ==========================================
   FDE ACCORDION CASE STUDY STYLES
   ========================================== */
.fde-accordion-page.fde-page {
  background-color: #ffffff !important;
}

.fde-accordion-page.fde-page main {
  background-color: #ffffff !important;
}

/* Reset all inherited styles */
.fde-accordion-page * {
  box-sizing: border-box !important;
}

.fde-accordion-page img {
  max-width: none !important;
}

/* Hero Section */
.fde-hero-section {
  padding: 100px 0 60px !important;
  background-color: #ffffff !important;
  border-bottom: 1px solid #e5e5e5 !important;
}

/* Logo container - sets the alignment baseline */
.fde-logo-container {
  max-width: 1200px !important;
  margin: 0 auto 32px !important;
  padding: 0 60px 24px !important;
  border-bottom: 1px solid #e5e5e5 !important;
}

.fde-logo-container img,
.fde-logo-container .fde-client-logo-img {
  width: 150px !important;
  height: auto !important;
}

/* Hero container - align to logo */
.fde-hero-container {
  max-width: 1200px !important;
  margin: 0 auto !important;
  display: grid !important;
  grid-template-columns: 1fr 450px !important;
  gap: 60px !important;
  align-items: start !important;
  padding: 0 60px !important;
}

.fde-hero-left {
  max-width: 600px !important;
}

.fde-hero-title {
  font-size: 96px !important;
  font-weight: 800 !important;
  line-height: 0.95 !important;
  letter-spacing: -3px !important;
  color: #1a1a1a !important;
  margin-bottom: 16px !important;
}

.fde-hero-subtitle {
  font-size: 20px !important;
  font-weight: 400 !important;
  line-height: 1.4 !important;
  color: #4a4a4a !important;
  margin-bottom: 32px !important;
}

.fde-hero-meta {
  border-top: 1px solid #e5e5e5 !important;
}

.fde-accordion-page .fde-hero-right img,
.fde-hero-right img {
  width: 450px !important;
  height: 260px !important;
  object-fit: cover !important;
  max-width: 450px !important;
  max-height: 260px !important;
}

.fde-hero-right {
  max-width: 450px !important;
  overflow: hidden !important;
}

/* Metrics Bar - align to logo */
.fde-metrics-bar {
  max-width: 1200px !important;
  margin: 48px auto 0 !important;
  display: grid !important;
  grid-template-columns: repeat(4, 1fr) !important;
  gap: 24px !important;
  padding: 32px 60px 0 !important;
  border-top: 1px solid #e5e5e5 !important;
}

.fde-metric-item {
  text-align: left !important;
}

.fde-metric-number {
  display: block !important;
  font-size: 36px !important;
  font-weight: 800 !important;
  color: #1a1a1a !important;
  line-height: 1 !important;
  margin-bottom: 8px !important;
}

.fde-metric-desc {
  font-size: 13px !important;
  color: #999 !important;
}

/* Accordion Wrapper - align to logo */
.fde-accordion-wrapper {
  max-width: 1200px !important;
  margin: 0 auto !important;
  padding: 0 60px 80px !important;
}

/* Full-width background for accordion area */
.fde-accordion-bg {
  background-color: #f7f7f7 !important;
  padding-top: 20px !important;
}

/* Accordion Section */
.fde-accordion-section {
  border-bottom: 1px solid #e5e5e5 !important;
}

.fde-accordion-header {
  width: 100% !important;
  display: flex !important;
  justify-content: space-between !important;
  align-items: center !important;
  padding: 32px 0 !important;
  background: none !important;
  border: none !important;
  cursor: pointer !important;
  text-align: left !important;
  transition: opacity 0.2s ease !important;
}

.fde-accordion-header:hover {
  opacity: 0.7 !important;
}

.fde-accordion-header-content {
  flex: 1 !important;
}

.fde-accordion-title {
  font-size: 48px !important;
  font-weight: 700 !important;
  color: #1a1a1a !important;
  margin: 0 0 8px 0 !important;
  line-height: 1 !important;
}

.fde-accordion-subtitle {
  font-size: 14px !important;
  color: #999 !important;
  margin: 0 !important;
}

.fde-accordion-icon {
  font-size: 32px !important;
  font-weight: 300 !important;
  color: #1a1a1a !important;
  width: 48px !important;
  height: 48px !important;
  display: flex !important;
  align-items: center !important;
  justify-content: center !important;
}

/* Accordion Content */
.fde-accordion-content {
  max-height: 0 !important;
  overflow: hidden !important;
  transition: max-height 0.4s ease, padding 0.4s ease !important;
}

.fde-accordion-content.open {
  max-height: 4000px !important;
  padding-bottom: 48px !important;
}

/* Two Column Layout */
.fde-accordion-page .fde-two-column,
.fde-two-column {
  display: grid !important;
  grid-template-columns: 1fr 450px !important;
  gap: 48px !important;
  align-items: start !important;
  width: 100% !important;
}

.fde-column-text h3 {
  font-size: 24px !important;
  font-weight: 700 !important;
  color: #1a1a1a !important;
  margin-bottom: 20px !important;
  line-height: 1.3 !important;
}

.fde-column-text p {
  font-size: 15px !important;
  line-height: 1.7 !important;
  color: #4a4a4a !important;
  margin-bottom: 16px !important;
}

.fde-accordion-page .fde-column-image img,
.fde-column-image img {
  width: 450px !important;
  height: 260px !important;
  object-fit: cover !important;
  max-width: 450px !important;
}

/* Next.js Image wrapper overrides */
.fde-accordion-page .fde-column-image,
.fde-accordion-page .fde-hero-right {
  width: 450px !important;
  flex-shrink: 0 !important;
}

/* Inline Metrics */
.fde-inline-metrics {
  display: flex !important;
  gap: 32px !important;
  margin-top: 24px !important;
  padding-top: 24px !important;
  border-top: 1px solid #e5e5e5 !important;
}

.fde-inline-metric {
  text-align: center !important;
}

.fde-inline-metric strong {
  display: block !important;
  font-size: 28px !important;
  font-weight: 800 !important;
  color: #1a1a1a !important;
}

.fde-inline-metric span {
  font-size: 12px !important;
  color: #999 !important;
}

/* Pipeline */
.fde-pipeline {
  display: flex !important;
  align-items: center !important;
  gap: 8px !important;
  flex-wrap: wrap !important;
  margin: 24px 0 16px !important;
}

.fde-pipeline .fde-pipeline-step {
  background-color: #f0f0f0 !important;
  padding: 8px 16px !important;
  font-size: 13px !important;
  font-weight: 500 !important;
  color: #1a1a1a !important;
}

.fde-pipeline .fde-pipeline-arrow {
  color: #ccc !important;
}

.fde-sources {
  font-size: 13px !important;
  color: #999 !important;
}

/* Capabilities */
.fde-capabilities {
  margin-top: 24px !important;
  padding-top: 24px !important;
  border-top: 1px solid #e5e5e5 !important;
}

.fde-capabilities div {
  margin-bottom: 12px !important;
  font-size: 14px !important;
  color: #4a4a4a !important;
}

.fde-capabilities strong {
  color: #1a1a1a !important;
}

/* Before/After */
.fde-before-after {
  display: grid !important;
  grid-template-columns: 1fr 1fr !important;
  gap: 32px !important;
  margin-top: 24px !important;
  padding-top: 24px !important;
  border-top: 1px solid #e5e5e5 !important;
}

.fde-before h4,
.fde-after h4 {
  font-size: 13px !important;
  font-weight: 600 !important;
  text-transform: uppercase !important;
  letter-spacing: 1px !important;
  margin-bottom: 12px !important;
}

.fde-before h4 {
  color: #999 !important;
}

.fde-after h4 {
  color: #1a1a1a !important;
}

.fde-before ul,
.fde-after ul {
  list-style: none !important;
  padding: 0 !important;
  margin: 0 !important;
}

.fde-before li,
.fde-after li {
  font-size: 14px !important;
  margin-bottom: 8px !important;
}

.fde-before li {
  color: #999 !important;
}

.fde-after li {
  color: #1a1a1a !important;
}

/* Impact Metrics */
.fde-impact-metrics {
  margin-top: 24px !important;
  padding-top: 24px !important;
  border-top: 1px solid #e5e5e5 !important;
}

.fde-impact-metric {
  display: flex !important;
  align-items: center !important;
  gap: 12px !important;
  margin-bottom: 16px !important;
  padding-bottom: 16px !important;
  border-bottom: 1px solid #f0f0f0 !important;
}

.fde-impact-metric:last-child {
  border-bottom: none !important;
  margin-bottom: 0 !important;
  padding-bottom: 0 !important;
}

.fde-impact-before {
  font-size: 16px !important;
  color: #999 !important;
  min-width: 80px !important;
}

.fde-impact-arrow {
  color: #ccc !important;
}

.fde-impact-after {
  font-size: 20px !important;
  font-weight: 700 !important;
  color: #1a1a1a !important;
  min-width: 80px !important;
}

.fde-impact-label {
  font-size: 13px !important;
  color: #999 !important;
  margin-left: auto !important;
}

/* Chart Section */
.fde-section-content {
  width: 100% !important;
}

.fde-chart-section {
  margin-top: 48px !important;
  padding-top: 32px !important;
  border-top: 1px solid #e5e5e5 !important;
}

.fde-chart-header {
  margin-bottom: 24px !important;
}

.fde-chart-header h4 {
  font-size: 18px !important;
  font-weight: 700 !important;
  color: #1a1a1a !important;
  margin: 0 0 8px 0 !important;
}

.fde-chart-header p {
  font-size: 14px !important;
  color: #666 !important;
  margin: 0 !important;
  line-height: 1.5 !important;
}

.fde-chart-container {
  background-color: #ffffff !important;
  border: 1px solid #e5e5e5 !important;
  padding: 24px !important;
  overflow-x: auto !important;
}

/* Responsive */
@media (max-width: 1024px) {
  .fde-hero-section {
    padding: 140px 0 48px !important;
  }
  
  .fde-logo-container {
    padding: 0 32px 24px !important;
  }
  
  .fde-hero-container {
    grid-template-columns: 1fr 380px !important;
    gap: 40px !important;
    padding: 0 32px !important;
  }
  
  .fde-hero-right {
    max-width: 380px !important;
  }
  
